  <Code>Public Function Spell(ByVal number As Integer) As String
		If number &gt; 999999999 Then Return "### Nominal terlalu besar. Maksimal nominal adalah 999.999.999 ###"
		Dim n, m As Integer
		Dim result As String = String.Empty
		n = Math.Floor(number / 1000000)
		m = number Mod 1000000
		If n &gt; 0 Then result += SpellHundreds(n) &amp; " Juta "
		n = Math.Floor(m / 1000)
		m = m Mod 1000
		If n &gt; 0 Then result += SpellHundreds(n) &amp; If(n = 1, " Seribu ", " Ribu")
		result += SpellHundreds(m) &amp; "Rupiah"
		Return System.Text.RegularExpressions.Regex.Replace(result, "\s+", " ").Trim()
	End Function

	Private Function GetHundreds(ByVal number As Integer) As String
		If number &lt;= 0 Then Return String.Empty
		Return If(number = 1, " Seratus", String.Concat(GetTeensUnit(number), " Ratus "))
	End Function

	Private Function SpellHundreds(ByVal number As Integer) As String
		Dim result As String = String.Empty
		Dim n As Integer
		n = Math.Floor(number / 100)
		number = number Mod 100
		result += GetHundreds(n)

		If number &gt;= 20 Then
			n = Math.Floor(number / 10)
			number = number Mod 10
			result += GetTens(n)
		End If

		result += GetTeensUnit(number)
		Return result
	End Function

	Private Function GetTens(ByVal number As Integer) As String
		Dim result As String = String.Empty

		Select Case number
			Case 2
				result = "  Dua Puluh "
			Case 3
				result = " Tiga Puluh "
			Case 4
				result = " Empat Puluh "
			Case 5
				result = " Lima Puluh "
			Case 6
				result = " Enam Puluh "
			Case 7
				result = " Tujuh Puluh "
			Case 8
				result = " Delapan Puluh "
			Case 9
				result = " Sembilan Puluh "
		End Select

		Return result
	End Function

	Private Function GetTeensUnit(ByVal number As Integer) As String
		Dim result As String = String.Empty

		Select Case number
			Case 1
				result = " Satu "
			Case 2
				result = " Dua "
			Case 3
				result = " Tiga "
			Case 4
				result = " Empat "
			Case 5
				result = " Lima "
			Case 6
				result = " Enam "
			Case 7
				result = " Tujuh "
			Case 8
				result = " Delapan "
			Case 9
				result = " Sembilan "
			Case 10
				result = " SePuluh  "
			Case 11
				result = " Sebelas "
			Case 12
				result = " Dua Belas "
			Case 13
				result = " Tiga Belas "
			Case 14
				result = " Empat Belas "
			Case 15
				result = " Lima Belas "
			Case 16
				result = " Enam Belas "
			Case 17
				result = " Tujuh Belas "
			Case 18
				result = " Delapan Belas "
			Case 19
				result = " Sembilan Belas "
			Case Else
				result = " "
		End Select

		Return result
	End Function</Code>
